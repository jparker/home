<Files .git>
  Order allow,deny
  Deny from all
</Files>

<Files .gitignore>
  Order allow,deny
  Deny from all
</Files>

<Files Capfile>
  Order allow,deny
  Deny from all
</Files>

<Files scotland.html>
  AuthUserFile .htpasswd
  AuthType Basic
  AuthName "Scotland/Ireland 2009"
  Require valid-user
</Files>
